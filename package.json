{
    "name": "slate-master",
    "version": "0.0.1",
    "scripts": {
        "watch": "concurrently \"gulp\" \"theme watch --env development --dir dist\" \"webpack serve -c ./webpack.dev.js\" --names \"gulp,themekit,webpack\" --prefix-colors \"#f04a71,cyan,blue\"",
        "watch:prod": "concurrently \"gulp\" \"theme watch --env prod --dir dist\" \"webpack serve -c ./webpack.dev.js\" --names \"gulp,themekit,webpack\" --prefix-colors \"#f04a71,cyan,blue\"",
        "start": "npm run watch",
        "deploy:qa": "theme deploy --env qa --dir dist --allow-live",
        "deploy:staging": "theme deploy --env staging --dir dist",
        "deploy:prod": "theme deploy --env prod --dir dist --allow-live",
        "deploy:next": "theme deploy --env next --dir dist",
        "build:qa": "webpack --config webpack.qa.js",
        "build:dev": "webpack --config webpack.dev.js",
        "build:staging": "npm run build:prod",
        "build:prod": "webpack --config webpack.prod.js",
        "prettier": "prettier --write src/{scripts,styles}",
        "clean": "npx unimported"
    },
    "resolutions": {
        "@babel/preset-env": "7.5.5"
    },
    "devDependencies": {
        "@babel/core": "7.13.15",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-transform-runtime": "7.13.15",
        "@babel/preset-env": "^7.13.15",
        "@shopify/slate-tools": "0.14.0",
        "@shopify/slate-translations": "^1.0.0-beta.19",
        "@webpack-cli/serve": "^1.4.0",
        "babel-loader": "8.2.2",
        "cache-loader": "^4.1.0",
        "concurrently": "6.0.2",
        "copy-webpack-plugin": "^8.1.1",
        "css-loader": "5.2.1",
        "css-minimizer-webpack-plugin": "^2.0.0",
        "eslint": "^7.24.0",
        "file-loader": "6.2.0",
        "gulp": "^4.0.2",
        "mini-css-extract-plugin": "1.4.1",
        "prettier": "^2.2.1",
        "sass": "1.32.8",
        "sass-loader": "11.0.1",
        "style-loader": "2.0.0",
        "terser-webpack-plugin": "5.1.1",
        "ts-loader": "^9.2.3",
        "typescript": "^4.3.4",
        "webpack": "5.33.2",
        "webpack-cli": "4.6.0",
        "webpack-dev-server": "^3.11.2",
        "yargs": "^16.2.0"
    },
    "dependencies": {
        "@babel/plugin-transform-react-jsx": "^7.13.12",
        "@babel/runtime": "7.13.10",
        "@vimeo/player": "2.15.0",
        "animate.css": "3.7.0",
        "aos": "^3.0.0-beta.6",
        "caniuse-lite": "^1.0.30001532",
        "classnames": "2.2.6",
        "dotenv": "^10.0.0",
        "enquire.js": "2.1.6",
        "hamburgers": "^1.1.3",
        "imports-loader": "2.0.0",
        "intersection-observer": "0.12.0",
        "jquery": "3.6.0",
        "jquery-hoverintent": "1.10.1",
        "jquery.panzoom": "3.2.3",
        "jquery.transit": "0.9.12",
        "scrolltofixed": "1.0.6",
        "slick-carousel": "1.8.1",
        "source-map": "^0.5.7",
        "underscore": "^1.13.1",
        "underscore-template-loader": "1.1.0",
        "vanilla-lazyload": "^17.3.1",
        "waypoints": "^4.0.1",
        "when-dom-ready": "1.2.12"
    },
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    }
}
